from sage.all import *
from Crypto.Util.number import long_to_bytes

# All variables are in hex
n = 

# Padding zeros to p --> p4
p4 = 0xe4e4b390c1d201dae2c00a4669c0865cc5767bc444f5d310f3cfc75872d96feb89e556972c99ae20753e3314240a52df5dccd076a47c6b5d11b531b92d901b2b512aeb0b263bbfd624fe3d52e5e238beeb581ebe012b2f176a4ffd1e0d2aa8c4d3a2656573b727d4d3136513a931428b00000000000000000000000000000000
cipher = 
e2 = 65537
kbits = 128  # kbits represent the loss bits
PR.<x> = PolynomialRing(Zmod(n))  # Create a polynomial ring
f = x + p4
roots = f.small_roots(X=2^kbits, beta=0.4)
if roots:        
    p = p4 + int(roots[0])
    assert n % p == 0
    q = n / int(p)
    phin = (p-1)*(q-1)
    d = inverse_mod(e2,phin)
    flag = pow(cipher,d,n)
    print long_to_bytes(flag)
